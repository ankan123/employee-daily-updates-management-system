        <a href="/admin" style="position:absolute;left:10%;top:19%;text-decoration:none;"><i class="fas fa-user-shield fa-2x"></i>Admin page</a>
  
      <div class="row mt-3">
      <div class="col-md-4 m-auto">
        <div class="card card-body">
       <h3>Employee Details</h3>
    <table class="table table-striped">
        <thread>
            <tr>
              
                <th>Name</th>
                <th>Employee-ID</th>
                <th></th>
            </tr>
        </thread>
        <tbody>
            <% for(var obj of collection)  { %>
